cod, qua = map(int, input().split())

if cod == 1:
    print(f'Total: R$ {(4.0 * qua):.2f}')
elif cod == 2:
    print(f'Total: R$ {(4.5 * qua):.2f}')
elif cod == 3:
    print(f'Total: R$ {(5.0 * qua):.2f}')
elif cod == 4:
    print(f'Total: R$ {(2.0 * qua):.2f}')
elif cod == 5:
    print(f'Total: R$ {(1.5 * qua):.2f}')
    