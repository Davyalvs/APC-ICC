m, d = input().split()
m, d = [int(m),int(d)]

if m == 2:
    if d > 1:
        print(5)
    else:
        print(4)
elif m == 1 or m == 3 or m == 5 or m == 7 or m == 8 or m == 10 or m == 12:
    if d > 5:
        print(6)
    else:
        print(5)
else:
    if d > 6:
        print(6)
    else:
        print(5)